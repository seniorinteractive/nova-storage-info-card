{
    "name": "qubeek/nova-storage-info-card",
    "description": "A Laravel Nova card, that helps you to control your S3 type disks.",
    "license": "MIT",
    "authors": [
        {
            "name": "qubeek",
            "email": "hello@qubeek.io",
            "homepage": "https://github.com/qubeek"
        }
    ],
    "homepage": "https://github.com/qubeek/nova-storage-info-card",
    "keywords": [
        "laravel",
        "nova",
        "storage",
        "S3",
        "card"
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Qubeek\\StorageInfoCard\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Qubeek\\StorageInfoCard\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
